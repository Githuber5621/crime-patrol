{
  "version": 2,
  "buildCommand": "npm ci --omit=optional && npm run build",
  "outputDirectory": "/dist",
  "installCommand": "npm ci --omit=optional",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
